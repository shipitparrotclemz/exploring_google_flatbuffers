namespace Parrot;

enum Color: byte { Red = 0, Green = 1, Blue = 2 }

enum Level: byte { One = 0, Two = 1, Three = 2, Four = 3, Five = 4 }

struct Position {
    x: float;
    y: float;
    z: float;
}

table Talent {
    name: string;
    level: Level;
}

table Parrot {
    position: Position;
    name: string;
    color: Color;
    talents: [Talent];
}

root_type Parrot;