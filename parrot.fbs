namespace Parrot;

attribute "priority";

enum Color: byte { Red = 1, Green = 2, Blue = 3 }

enum Level: byte { One = 1, Two = 2, Three = 3, Four = 4, Five = 5 }

struct Position {
    x: float;
    y: float;
    z: float;
}

table Talent {
    name: string;
    level: Level;
}

table Parrot {
    position: Position;
    name: string;
    color: Color;
    talents: [Talent];
}

root_type Parrot;